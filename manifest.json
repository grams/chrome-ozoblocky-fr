{
  "name": "OzoBlockly en Français",
  "version": "1.0.0",
  "description": "Traduit en Français l'éditeur OzoBlockly",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://ozoblockly.com/editor?*"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "icons": {
    "16": "images/ozoblocky-fr16.png",
    "32": "images/ozoblocky-fr32.png",
    "48": "images/ozoblocky-fr48.png",
    "128": "images/ozoblocky-fr128.png"
  }
}
